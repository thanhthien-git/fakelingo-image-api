{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "**/*.func": { "maxDuration": 60, "memory": 1024, "maxBodySize": "20mb" }
  }
}
